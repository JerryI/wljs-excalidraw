{
    "name": "wljs-excalidraw",
    "version": "0.0.4",
    "description": "Integration of Excalidraw",
    "scripts": {
        "build": "node --max-old-space-size=8192 ./node_modules/.bin/rollup --config rollup.config.mjs",
        "restore": "mkdir _temp && cd _temp && git clone https://github.com/JerryI/wljs-excalidraw && mv wljs-excalidraw/.git ../.git && cd .. && rm -rf _temp"
    },
    "defaultPackage": true,
    "wljs-meta": {
        "priority": 800,
        "category": "Cell types",
        "js": "dist/kernel.js",
        "minjs": "dist/kernel.js",
        "frontend" : "src/Frontend.wl",
        "styles": "src/styles.css"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/JerryI/wljs-excalidraw"
    },
    "dependencies": {
        "@codemirror/view": "^6.28.6",
        "@excalidraw/excalidraw": "^0.17.6",
        "@rollup/plugin-terser": "^0.4.4",
        "excalidraw": "^0.6.4",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "rollup": "^3.21.6"
    },
    "resolutions": {
        "@babel/preset-env": "7.13.8"
    },
    "devDependencies": {
        "react-error-overlay": "^6.0.9",
        "rollup-plugin-replace": "^2.2.0"
    }
}
